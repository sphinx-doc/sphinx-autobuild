[run]
branch = True
omit = setup.py, */migrations/*, */conftest.py, docs/*

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    NOCOV

    # Don't complain if non-runnable code isn't run:
    if __name__ == .__main__.:

[html]
directory = .htmlcov
